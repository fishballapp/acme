{
  "name": "@fishballpkg/acme",
  "version": "0.7.1",
  "exports": {
    ".": "./src/mod.ts",
    "./DnsUtils": "./src/DnsUtils/mod.ts",
    "./generateCSR": "./src/utils/generateCSR.ts",
    "./workflows": "./src/AcmeWorkflows.ts"
  },
  "tasks": {
    "test:e2e": "deno test -A e2e",
    "test:unit": "deno test -A src",
    "test:integration": "deno test --unsafely-ignore-certificate-errors=localhost -A integration",
    "pebble:start": "echo ghcr.io/letsencrypt/pebble:latest | xargs env PEBBLE_VA_NOSLEEP=1 docker run -p 14000:14000 -p 15000:15000 --rm",
    "build:npm": "deno run -A scripts/build-npm.ts"
  },
  "compilerOptions": {
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "useUnknownInCatchVariables": true,
    "noFallthroughCasesInSwitch": true
  },
  "publish": {
    "include": ["src", "LICENSE", "README.md"],
    "exclude": ["**/*.test.ts"]
  },
  "exclude": ["dist*"],
  "imports": {},
  "license": "MIT"
}
